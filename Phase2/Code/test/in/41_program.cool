class Sort {
  func bubbleSort (int[] items) int[] {
    let int i;
    let int j;
    let int n;
    n = len(items);
    for (i = 0 ; i < n- 1 ; i = i + 1)
      for (j = 0 ; j < n - i - 1 ; j = j + 1) {
        if (items[j] > items[j+ 1]) {
          let int t;
          t = items[j];
          items[j] = items[j + 1];
          items[j + 1] = t;
        }
      }
    return items;
  }
}


class Main {
  let int[] items;
